<form class="{{cssClass}}" autocomplete="off">
    <header class="sheet-header">
        <div class="nameLable">
            <select class="input165" name="system.skill">
                {{#select system.skill}}
                    {{#each config.ritualSkills as |name type|}}
                        <option value="{{type}}">{{localize name}}</option>
                    {{/each}}
                {{/select}}
            </select>
        </div>
    </header>
</form>