			    <div class="smallValueBox editableValue">0</div>
			    <div class="smallValueBox"></div>
		    </div>
		    <div class="valueDivider"></div>
		    <div class="valueBox2">
		    	<div class="bigValueBox"></div>
	    		<div class="smallValueBox valueHead">Max</div>
    			<div class="smallValueBox valueHead">Wert</div>
			    <div class="smallValueBox valueHead">Mod</div>
		    	<div class="smallValueBox"></div>
	    		<div class="smallValueBox"></div><br />
    			<div class="bigValueBox">Lebenspunkte</div>
			    <div class="smallValueBox">36</div>
		    	<div class="smallValueBox">36</div>
	    		<div class="smallValueBox editableValue">0</div>
    			<div class="smallValueBox">+</div>
			    <div class="smallValueBox">-</div><br />
		    	<div class="bigValueBox">Ausdauer</div>
	    		<div class="smallValueBox">36</div>
    			<div class="smallValueBox">36</div>
			    <div class="smallValueBox editableValue">0</div>
		    	<div class="smallValueBox">+</div>
	    		<div class="smallValueBox">-</div><br />
    			<div class="bigValueBox">Astralenergie</div>
			    <div class="smallValueBox">36</div>
			    <div class="smallValueBox">36</div>
		    	<div class="smallValueBox editableValue">0</div>
	    		<div class="smallValueBox">+</div>
    			<div class="smallValueBox">-</div><br />
			    <div class="bigValueBox">Karmaenergie</div>
		    	<div class="smallValueBox">36</div>
