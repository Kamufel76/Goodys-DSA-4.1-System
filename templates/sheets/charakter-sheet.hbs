<form class="{{cssClass}}" autocomplete="off">
    <div class="sheet">
	    <div class="menuBar">
			{{> "systems/GDSA/templates/partials/character-sheet-menu.hbs"}}
	    </div>
	    <div class="fillerHead"></div>
	    <div class="infoBox">
		    <div class="imgBox">
			    <img class="img" src="{{actor.img}}" data-edit="img" title="{{actor.name}}" />
		    </div>
		    <div class="infoDivider"></div>
	    	<div class="factBox">
			    <div class="factType">Name:</div><div data-edit="name" class="factValue editableValue" contenteditable>{{actor.name}}</div><br />
		    	<hr class="factDivider" />
	    		<div class="factType">Rasse:</div><div class="factValue">{{data.race}}</div><br />
    			<hr class="factDivider"  />
			    <div class="factType">Kultur:</div><div class="factValue">{{data.kulture}}</div><br />
		    	<hr class="factDivider"  />
	    		<div class="factType">Profession:</div><div class="factValue">{{data.profession}}</div><br />
    			<hr class="factDivider"  />
			    <div class="factType">Geschlecht:</div><div data-edit="data.gender" class="factValue editableValue" contenteditable>{{data.gender}}</div><br />
		    	<hr class="factDivider"  />
	    		<div class="factType">Alter:</div><div data-edit="data.age" class="factValue editableValue" contenteditable>{{data.age}}</div><br />
    			<hr class="factDivider"  />
			    <div class="factType">Größe:</div><div data-edit="data.height" class="factValue editableValue" contenteditable>{{data.height}}</div><br />
		    	<hr class="factDivider"  />
	    		<div class="factType">Gewicht:</div><div data-edit="data.weight" class="factValue editableValue" contenteditable>{{data.weight}}</div><br />
    			<hr class="factDivider"  />
			    <div class="factType">Sozialstatus:</div><div data-edit="data.SO" class="factValue editableValue" contenteditable>{{data.SO}}</div>
		    </div>
	    </div>
    	<div class="filler"></div>
	    <div class="valueBox">
		    <div class="flawBox">
				<div class="flawhead">Vorteile</div>
				<hr class="flawhr" />
			    <section class="advantages">
					{{#each advantages as |advantage id|}}
					<div class="flaw">{{advantage.name}}</div>
					<hr class="flawhr" />
					{{/each}}
				</section>
		    </div>
		    <div class="flawDivider"></div>
		    <div class="flawBox">
				<div class="flawhead">Nachteile</div>
				<hr class="flawhr" />
		    </div>
	    </div>	
	    <div class="filler"></div>	
	    <div class="valueBox">
		    <div class="valueBox1">
			    <div class="bigValueBox"></div>
				<div class="smallValueBox valueHead">Wert</div>
				<div class="smallValueBox valueHead">Mod.</div>
				<div class="smallValueBox"></div><br />
			    <div class="bigValueBox">Mut</div>
			    <div class="smallValueBox">{{data.MU.value}}</div>
			    <div class="smallValueBox editableValue" data-edit="data.Stats.MU.temp" contenteditable>{{data.Stats.MU.temp}}</div>
			    <div class="smallValueBox">roll</div><br />
			    <div class="bigValueBox">Klugheit</div>
			    <div class="smallValueBox">12</div>
			    <div class="smallValueBox editableValue">0</div>
			    <div class="smallValueBox">roll</div><br />
