<form class="{{cssClass}}" autocomplete="off">

    <div class="browserImgBox3"><img class="browserImg" name="{{item.name}}" data-edit="img" src="{{item.img}}" /></div>

    <div class="ritHelper7">
        <div class="spHelper1">{{localize "GDSA.itemsheet.name"}}</div>
        <div class="spHelper1"><input class="spHelper6" name="name" type="text" value="{{item.name}}" /></div>
    </div>
    
    <div class="itemRow3">
        <div class="spHelper8">{{localize "GDSA.itemsheet.lvl"}}</div> <div class="spHelper3"><input class="spHelper3" name="system.grad" type="text" value="{{system.grad}}" /></div>
        <div class="spHelper8">PDF</div> <div class="spHelper3"><input class="spHelper3" name="system.lltPage" type="text" value="{{system.lltPage}}" /></div>
        <div class="spHelper8">{{localize "GDSA.chat.medi.target"}}</div> <div class="spHelper3">
            <select class="spHelper3" name="system.target">
                    {{#select system.target}}
                        {{#each config.wonderTarget as |name|}}
                            <option value="{{name}}">{{name}}</option>
                        {{/each}}
                    {{/select}}
            </select>
        </div>
    </div>

    <nav class="tabs nav spell-tabs" data-group="spell">
        <a class="item" data-tab="spellDetails"><div class="spellMenu2">Details</div></a>  
        <a class="item" data-tab="spellVariants"><div class="spellMenu">Verbreitung</div></a>
        <a class="item" data-tab="spellEffects"><div class="spellMenu">Effekte</div></a>
        <a class="item" data-tab="spellNotes"><div class="spellMenu">Notizen</div></a>
    </nav>

    <div class="spellMenuBox2">
	    <section class="spell-body">

            <div class="tab spellDetails" data-group="spell" data-tab="spellDetails">

                <div class="input100 spellName">{{localize "GDSA.chat.medi.ritDura"}}:</div>
                <select class="input70sa" name="system.ritduaration">
                    {{#select system.ritduaration}}
                        {{#each config.wonderCastDuration as |name type|}}
                            <option value="{{type}}">{{localize name}}</option>
                        {{/each}}
                    {{/select}}
                </select>
                <input class="ritHelper6" name="system.ritshortdura" type="text" value="{{system.ritshortdura}}" />

                <div class="input100 spellName">{{localize "GDSA.chat.medi.wirkDur"}}:</div>
                <select class="input70s" name="system.duration">
                    {{#select system.duration}}
                        {{#each config.wonderDuration as |name type|}}
                            <option value="{{name}}">{{name}}</option>
                        {{/each}}
                    {{/select}}
                </select>

                <div class="input100 spellName">{{localize "GDSA.chat.medi.rangeH"}}:</div>
                <select class="input70s" name="system.range">
                    {{#select system.range}}
                        {{#each config.wonderRange as |name type|}}
                            <option value="{{type}}">{{localize name}}</option>
                        {{/each}}
                    {{/select}}
                </select>

                <div class="input100 spellName">Wirkungsst√§rke:</div>
                <select class="input70s" name="system.power">
                    {{#select system.power}}
                        {{#each config.wonderPower as |name|}}
                            <option value="{{name}}">{{name}}</option>
                        {{/each}}
                    {{/select}}
                </select>

            </div>

            <div class="tab spellVariants" data-group="spell" data-tab="spellVariants">
                <div class="spHelper9">{{localize "GDSA.holyreps.pra"}}</div><div class="spHelper10"><input class="mwHelper6" name="system.verb.Pra" type="checkbox" {{checked system.verb.Pra}} /></div>
                <div class="spHelper9">{{localize "GDSA.holyreps.ron"}}</div><div class="spHelper10"><input class="mwHelper6" name="system.verb.Ron" type="checkbox" {{checked system.verb.Ron}} /></div>
                <div class="spHelper9">{{localize "GDSA.holyreps.phx"}}</div><div class="spHelper10"><input class="mwHelper6" name="system.verb.Phx" type="checkbox" {{checked system.verb.Phx}} /></div>

