                <div class="spHelper9">{{localize "GDSA.holyreps.grv"}}</div><div class="spHelper10"><input class="mwHelper6" name="system.verb.Grv" type="checkbox" {{checked system.verb.Grv}} /></div>
                <div class="spHelper9">{{localize "GDSA.holyreps.him"}}</div><div class="spHelper10"><input class="mwHelper6" name="system.verb.Him" type="checkbox" {{checked system.verb.Him}} /></div>
                
                <div class="spHelper9">{{localize "GDSA.holyreps.zsa"}}</div><div class="spHelper10"><input class="mwHelper6" name="system.verb.Zsa" type="checkbox" {{checked system.verb.Zsa}} /></div>
