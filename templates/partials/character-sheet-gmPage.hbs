<section>
    <div class="gmBox">
        <table>
            <tr>
                <td class="gmTableHelper">{{localize "GDSA.charactersheet.InvEdit"}}</td>
                <td><input name="system.editInv" type="checkbox"  {{checked system.editInv}} /></td>
            </tr>
        </table>

        <table class="CharLog">
            <tr>
                <td class="gmTableHelper2">{{localize "GDSA.system.users"}}</td>
                <td class="gmTableHelper2">{{localize "GDSA.system.actions"}}</td>
                <td class="gmTableHelper2">{{localize "GDSA.system.date"}}</td>
                <td class="gmTableHelper2">{{localize "GDSA.system.time"}}</td>
                <td class="gmTableHelper2">{{localize "GDSA.system.element"}}</td>
            </tr>
            {{#each system.log}}
                <tr>
                    <td class="gmTableHelper2"><usr title="{{userId}}">{{userName}}</usr></td>
                    <td class="gmTableHelper2">{{action}}</td>
                    <td class="gmTableHelper2">{{date}}</td>
                    <td class="gmTableHelper2">{{time}}</td>
                    <td class="gmTableHelper2"><ele title="{{elementType}}">{{elementName}}</ele></td>
                </tr>
            {{/each}}
        </table>

        <h2>{{localize "fs2e.sheet.effects"}}
        <a class="effect-control" data-action="create">
          <i class="fas fa-plus"></i>
        </a>
      </h2>
      <ol>
        {{#each effects as |effect|}}
        <li class="item effect" data-effect-id="{{effect._id}}">
          <div class="item-name effect-name">
            <h4>{{effect.data.label}}</h4>
          </div>
          <div class="effect-controls">
            <a class="effect-control" data-action="edit" title="{{localize 'DND5E.EffectEdit'}}">
              <i class="fas fa-edit"></i>
            </a>
            <a class="effect-control" data-action="delete" title="{{localize 'DND5E.EffectDelete'}}">
              <i class="fas fa-trash"></i>
            </a>
          </div>
        </li>
        {{/each}}
      </ol>
    </div>
</section>