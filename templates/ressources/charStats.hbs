<form class="dialogConfig">
    <div class="form-group subMain">
        <div class="submen"><hr class="subhr" /></div>{{name}}<div class="submen"><hr class="subhr" /></div>
    </div>

    <div class="dialogFiller"></div>

    <div class="form-group">
        <label>{{localize "GDSA.charactersheet.race"}}</label>
        <input type="text" name="race" value="{{race}}" />
    </div>
    <div class="form-group">
        <label>{{localize "GDSA.charactersheet.culture"}}</label>
        <input type="text" name="culture" value="{{culture}}" />
    </div>
    <div class="form-group">
        <label>{{localize "GDSA.charactersheet.profession"}}</label>
        <input type="text" name="profession" value="{{profession}}" />
    </div>
    <div class="form-group">
        <label>{{localize "GDSA.charactersheet.gender"}}</label>
        <input type="text" name="gender" value="{{gender}}" />
    </div>
    <div class="form-group">
        <label>{{localize "GDSA.charactersheet.age"}}</label>
        <input type="number" name="age" value="{{age}}" />
    </div>
    <div class="form-group">
        <label>{{localize "GDSA.charactersheet.height"}}</label>
        <input type="number" name="size" value="{{size}}" />
    </div>
    <div class="form-group">
        <label>{{localize "GDSA.charactersheet.weight"}}</label>
        <input type="number" name="weight" value="{{weight}}" />
    </div>
    <div class="form-group">
        <label>{{localize "GDSA.charactersheet.so"}}</label>
        <input type="number" name="social" value="{{social}}" />
    </div>
    
    <div class="dialogFiller"></div>

    <div class="form-group subSub">
        <div class="submen"><hr class="subhr" /></div><div class="submen"><hr class="subhr" /></div>
    </div>

    <div class="dialogFiller"></div>
    
</form>